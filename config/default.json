{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "DB_NAME": "lel_manager",
  "DB_USERNAME": "postgres",
  "DB_PASSWORD": "postgres",
